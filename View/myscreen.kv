#:import Factory kivy.factory.Factory
#:import Window kivy.core.window

<MyField@MDTextField>
    size_hint_x: None
    width: '200'
    text_color: "000000"

    required: True
    helper_text: "This field is required"
    helper_text_mode: "on_error"

    color_mode: 'custom'
    line_color_focus: 'c06ef0'

    multiline: False
    background_color: (1, 1, 1)
    background_normal:''


<TooltipCheckBox@ToolTip+CheckBox>
    tooltip_bg_color: [192/256,110/256,240/256,1]
<TooltipButton@ToolTip+Button>
    tooltip_bg_color: [192/256,110/256,240/256,1]

<DropDownItem@MDDropDownItem>:
    Button:
        text: 'first'
        size_hint_y: None
        height: 40
        on_release: root.select('item1')



<AddPopup@Popup>
    auto_dismiss: False
    title: 'Регистрация'
    separator_color: (192/256,110/256,240/256,1)
    size_hint:None,None
    size:(400,550)
#    background_color:'CC99FF'
#    background_normal:''

    BoxLayout:
        # background color
        canvas.before:
            Color:
                rgb: (1,1,1)
            Rectangle:
                pos: self.pos
                size: self.size



        orientation: "vertical"
        adaptive_size: True

        #spacing: "12dp"
        #padding: "12dp"
        #radius: [10, ]
        #pos_hint: {"center_x": .5, "center_y": .5}
        GridLayout:
            cols:2
            Label:
                text: "[color=c06ef0]                                  Sing UP[/color]"
                markup: True
                font_size: 20
                size_hint: None, None
                height:50
                width:50
                pos_hint: {"center_x": .5}

            Image:
                source:'ilopet.jpg'
#                center_x:self.parent.center_x
#                center_y:self.parent.center_y
                size:"20sp","20sp"
        GridLayout:
            cols:3
            rows:1
            size_hint_y:None
            height:50
            padding:75,0,10,0
           # spacing:10
        # имя пациента
            MyField
                id: pet_name_input
                hint_text: 'Имя пациента'
                on_text: root.set_pet_name(self.text)

                pos_hint: {"center_x": .30}

            Label:
                id:click_label
            Spinner:
                id:spinner_id
                text:"Вид"
                values: root.get_values()
                size_hint:None,None
                size:(70,30)
                on_text:root.set_pet_type(spinner_id.text)
                background_color:"c06ef0"
                background_normal:''


        GridLayout:
            cols:2
            padding:75,0,0,0

        # дата рождения
            MyField:
                id: birth_date_input
                hint_text: 'Дата рождения'
                helper_text: 'YYYY-MM-DD'
                helper_text_mode: 'on_focus'
                max_text_length: 10
                on_text: root.set_birth_date(self.text)

            #Button:
            TooltipButton:
                tooltip_text: 'календарь'
                size_hint:None,None
#            text:'Дата рождения'
#            front_size:32
                size: "40sp", "40sp"
                on_press: root.choose_birth_date()
                background_color:0,0,0,0

                Image:
                    source:'i.jpg'
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size:"40sp","40sp"

            # дата последнего приёма
            MyField:
                id: last_appointment_date_input
                hint_text: 'Дата последнего посещ-ия'
                helper_text: 'YYYY-MM-DD'
                helper_text_mode: 'on_focus'
                max_text_length: 10
                on_text: root.set_last_appointment_date(self.text)
#                pos_hint: {"center_x": .5,"center_y":.5}

            TooltipButton:
#                text:'Дата последнего приёма'
                tooltip_text: 'календарь'
                size_hint: None, None
                size: "40sp", "40sp"
                on_press: root.choose_last_appointment_date()
                background_color:0,0,0,0

                Image:
                    source:'i.jpg'
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size:"40sp","40sp"
        GridLayout:
            cols: 1
            padding: 75,0,0,0
            # имя врача
            MyField:
                hint_text: 'ФИО врача'
                id: vet_name_input
                on_text: root.set_vet_name(self.text)
                multiline: False
                pos_hint: {"center_x": .5,"center_y":.5}

            # название болезни
            MyField:
                hint_text: 'Диагноз'
                id: disease_input
                on_text: root.set_disease(self.text)
                multiline: False
                pos_hint: {"center_x": .5,"center_y":.5}

        GridLayout:
            cols: 1
            rows: 2
            padding: 100, 0, 0, 0
            spacing: 10
            MDFillRoundFlatButton:
                text: '  Добавить информацию  '
                #disabled: True if root.everything() else False
                on_press: root.record_patient_info()
                #on_release: root.dialogs()
                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 0, 0, 0, 1
    #            background_color: 'CCCCFF'
    #            background_normal:''
                pos_hint: {"center_x": .5,"center_y":.5}



            MDFillRoundFlatButton:
                text: 'Вернуться в главное меню'
                #disabled: True if root.everything() else False
                on_press: root.dismiss()
                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 1, 0, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}


<SearchPopup@Popup>
