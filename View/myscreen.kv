                on_press: root.choose_search_birth_date()
                background_color:0,0,0,0
                Image:
                    source:'i.jpg'
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size:"40sp","40sp"

        GridLayout:
            cols: 2
            padding:50,0,0,0
            #имя врача
            MyField:
                hint_text: 'Имя врача'
                id: vet_name
                on_text: root.set_search_vet_name(self.text)
                multiline: False
            #CheckBox:
            TooltipCheckBox:
                tooltip_text: 'по ФИО врача и\nдате последнего посещения'
                group: 'search'
                color: '330000'
                size_hint: None, None
                height: 30
                width: 200
                on_active: root.set_properties(self, self.active, 'vet_name', 'last_appointment_date')
        GridLayout:
            cols: 2
            padding: 50,0,0,0
            spacing:20
            MyField:
                id: last_appointment_date_search
                hint_text: 'Дата посленего посещ-ия'
                helper_text: 'YYYY-MM-DD'
                helper_text_mode: 'on_focus'
                max_text_length: 10
                on_text: root.set_search_last_appointment_date(self.text)
                multiline: False
            TooltipButton:
                tooltip_text: 'календарь'
                size_hint: None, None
                size: "40sp", "40sp"
                on_press: root.choose_search_last_appointment_date()
                background_color:0,0,0,0
                Image:
                    source:'i.jpg'
                    center_x:self.parent.center_x
                    center_y:self.parent.center_y
                    size:"40sp","40sp"

        GridLayout:
            cols: 2
            padding: 50,0,0,0
            MyField:
                id: disease
                hint_text: 'Диагноз'
                on_text: root.set_search_disease_phrase(self.text)
                multiline: False

            #CheckBox:
            TooltipCheckBox:
                tooltip_text: 'по фразе из\nдиагноза'
                group: 'search'
                color: '330000'
                size_hint: None, None
                height: 30
                width: 200
                on_active: root.set_properties(self, self.active, 'disease', '')


        GridLayout:
            cols: 2
            padding: 20, 0, 0, 0
            spacing: 10
            MDFillRoundFlatButton:
                text: '          Поиск             '
                #disabled: True if root.everything() else False
                on_press: root.search()
                #on_release: root.dialogs()
                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 0, 0, 0, 1
    #            background_color: 'CCCCFF'
    #            background_normal:''
                pos_hint: {"center_x": .5,"center_y":.5}



            MDFillRoundFlatButton:
                text: 'Вернуться в главное меню'
                #disabled: True if root.everything() else False
                on_press: root.dismiss()
                #on_press: root.info()
                text_color: 0, 0, 0, 1
                line_color: 1, 0, 1, 1
                pos_hint: {"center_x": .5,"center_y":.5}


