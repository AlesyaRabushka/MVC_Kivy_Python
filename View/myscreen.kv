<WarningPopup@Popup>
    title: 'XML Parsing Error'
    auto_dismiss:False
    separator_color: (192/256,110/256,240/256,1)
    background_color:'#a910e6'
    size_hint:None,None
    size:(350,450)
    pos_hint: {"center_x": .5,"center_y":.5}

    BoxLayout:
        # background color
#        canvas.before:
#            Color:
#                rgb: (1,1,1)
#            Rectangle:
#                pos: self.pos
#                size: self.size



        orientation: "vertical"
        adaptive_size: True

        Label:
            text: 'Something went wrong'
        GridLayout:
            cols:2
            Label:
                text: 'Amount:'
            Label:
                id: files_count
        GridLayout:
            cols:2
            Label:
                text: 'Name:'
            Label:
                id: line_name
        GridLayout:
            cols:2
            Label:
                text: 'Count:'
            Label:
                id: line_count

        Button:
            text: 'do you want to fix them?'
            on_press: root.dismiss()






